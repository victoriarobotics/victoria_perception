<launch>
	<arg name="doDebug" default="false" />
	<arg if="$(arg doDebug)" name="launchPrefix" value="xterm -e gdb -args" />
	<arg unless="$(arg doDebug)" name="launchPrefix" value="" />

	<node pkg="victoria_perception" name="kmeans_serice" type="kmeans_service_node" respawn="true" output="screen" launch-prefix="$(arg launchPrefix)" >
	</node>
</launch>